{
  "functions": {
    "api/server.js": {
      "runtime": "nodejs@16.0.0"
    }
  },
  "builds": [
    {
      "src": "public/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    { "src": "/api/camStat", "dest": "/api/server.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
