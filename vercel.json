{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@now/static"
    },
    {
      "src": "server.js",
      "use": "@now/node"
    },
    {
      "src": "/camStat",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/$1" 
    }
  ]
}
